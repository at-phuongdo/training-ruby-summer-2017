  <tr>
    <td><%= image_tag user.image.thumb.url if user.image? %></td>
    <td><%= user.name %></td>
    <td><%= user.user_name %></td>
    <td><%= user.email %></td>
    <td><%= user.birthday %></td>
    <td><%= user.gender %></td>
    <td><%= link_to 'Show', user_path(user.id), :class => 'link-to' %></td>
    <td><%= link_to 'EditUser', edit_user_path(user), :class => 'link-to' %></td>
    <td><%= link_to 'Delete', user_path(user.id),
      method: :delete,
      data: { confirm: 'Are you sure?' } ,:class => 'link-to'%></td>
  </tr>